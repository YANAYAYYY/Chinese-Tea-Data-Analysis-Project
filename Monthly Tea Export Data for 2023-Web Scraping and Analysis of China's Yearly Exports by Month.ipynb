    "#建立函数：输入每个月链接后获得文字信息。\n",
    "def url(link):\n",
    "    text = []\n",
    "    response02 = requests.get(f\"https://www.ctma.com.cn{link}\", headers=headers)\n",
    "    html02 = response02.text\n",
    "    soup02 = BeautifulSoup(html02, \"html.parser\")\n",
    "    all_texts = soup02.find_all(\"p\")\n",
    "    for texts in all_texts:\n",
    "        text_contents = texts.get_text().strip()\n",
    "        text.append(text_contents)\n",
    "    text = ''.join(text)\n",
    "    return text\n",
    "\n",
    "#中国各类茶叶出口量函数：输入网页链接，将其转化成数据\n",
    "def tea_type(link):\n",
    "    url(link)\n",
    "    data = []\n",
    "    pattern0 = r\"(\\d+)年\"\n",
    "    pattern1 = r\"(\\d+|1-)月\"\n",
    "    pattern2 = r'绿茶出口量为(\\d+(\\.\\d+)?(万)?)吨'\n",
    "    pattern3 = r\"红茶出口量为(\\d+(\\.\\d+)?(万)?)吨\"\n",
    "    pattern4 = r\"乌龙茶出口量为(\\d+(\\.\\d+)?(万)?)吨\"\n",
    "    pattern5 = r\"茉莉花茶出口量为(\\d+(\\.\\d+)?(万)?)吨\"\n",
    "    pattern6 = r\"普洱茶出口量为(\\d+(\\.\\d+)?(万)?)吨\"\n",
    "    pattern7 = r\"其他花茶出口量为(\\d+(\\.\\d+)?(万)?)吨\"\n",
    "    pattern8 = r\"黑茶出口量为(\\d+(\\.\\d+)?(万)?)吨\"\n",
    "    pattern9 = r\"白茶出口量为(\\d+(\\.\\d+)?(万)?)吨\"\n",
    "    Search_Pattern = [pattern0,pattern1,pattern2,pattern3,pattern4,pattern5,pattern6,pattern7,pattern8,pattern9]\n",
    "    content =  ['年','月份','绿茶', '红茶', '乌龙茶', '茉莉花茶', '普洱茶', '其他花茶', '黑茶', '白茶']\n",
    "    for pattern in Search_Pattern:\n",
    "        match = re.search(pattern, url(link))\n",
    "        if match:\n",
    "            value = match.group(1)\n",
    "            #将其中带“万\"单位的数字x10000并去掉单位重新生成新的列表，不带“万”单位的数字转换成浮点数格式，便于后续计算\n",
    "            if \"万\" in value:\n",
    "                number = float(value[:-1]) * 10000\n",
    "            else:\n",
    "                number = float(value)\n",
    "            data.append(number)\n",
    "\n",
    "    #2023年中国各类茶叶出口量\n",
    "    df_tea_type_2023 = pd.DataFrame(list(zip(content[2:], data[2:])), columns=[\"茶类\", f\"{int(data[0])}年{int(data[1])}月累计出口量（吨）\"])\n",
    "    return df_tea_type_2023\n",
    "\n",
    "#中国茶叶出口量和出口额函数：输入识别到的文本，将其转化成数据\n",
    "def tea_export(link):\n",
    "    url(link)\n",
    "    data = []\n",
    "    pattern0 = r\"(\\d+)年\"\n",
    "    pattern1 = r\"(\\d+)月\"\n",
    "    pattern10 = r\"中国茶叶出口量总计为(\\d+(\\.\\d+)?(万)?)吨\"\n",
    "    pattern11 = r\"中国茶叶出口额为(\\d+(\\.\\d+)?)亿美元\"     #(?!累计)表示出口额不能紧跟累计\n",
    "    Search_Pattern = [pattern0,pattern1,pattern10,pattern11]\n",
    "    content =  ['年','月份','中国茶叶累计出口量(吨)','累计出口额（亿美元）']\n",
    "    for pattern in Search_Pattern:\n",
    "        match = re.search(pattern, url(link))\n",
    "        if match:\n",
    "            value = match.group(1)\n",
    "            #将其中带“万\"单位的数字x10000并去掉单位重新生成新的列表，不带“万”单位的数字转换成浮点数格式，便于后续计算\n",
    "            if \"万\" in value:\n",
    "                number = float(value[:-1]) * 10000\n",
    "            else:\n",
    "                number = float(value)\n",
    "            data.append(number)\n",
    "            \n",
    "    #2023年中国茶叶出口量和出口额汇总\n",
    "    df_tea_export_2023 = pd.DataFrame([data],columns = content)\n",
    "    df_tea_export_2023[\"年\"] = df_tea_export_2023[\"年\"].astype(int)\n",
    "    df_tea_export_2023[\"月份\"] = df_tea_export_2023[\"月份\"].astype(int)\n",
    "    return df_tea_export_2023"
   ]
  },
